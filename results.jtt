app.groupSize = 1;

let stage = app.newStage('results');
stage.playerStart = function(player) {
    player.chosenBlock = Utils.randomInt(0, player.participant.payoffs.length);
    player.points = player.participant.payoffs[player.chosenBlock].payoff;
}
stage.activeScreen = `
    <h4>End</h4>
    <p>The computer has now randomly determined your payoff for this experiment. Your payoff is: {{player.points}}.</p>
    <br>
`